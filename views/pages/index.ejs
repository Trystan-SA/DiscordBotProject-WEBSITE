    <css-doodle style="z-index: 1; position:realtive; height:1000px;"> 
            :doodle {
            @grid: 1x3 / 100vmax;
            position: fixed;
            top: 0; left: 0;
            }
        
            position:relative;

            background: @m(55, (
                linear-gradient(transparent, @p(
            #348ff9@repeat(2, @p([0-9a-f])),
            #9334f9@repeat(2, @p([0-9a-f]))
            ))
                @r(0%, 100%) @r(0%, 300%) /
                @r(3px) @r(20vmin)
                no-repeat
            ));
        
            will-change: transform;
            animation: f 20s linear calc(-200s / @size() * @i()) infinite;
            @keyframes f {
                from { transform: translateY(-100%) }
                to { transform: translateY(100%) }
            }
        </css-doodle>
